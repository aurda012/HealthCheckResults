<p id="notice"><%= notice %></p>

<h1>Survey Results</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Name</th>
      <th>Title</th>
      <th>Company</th>
      <th>Phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @survey_results.each do |survey_result| %>
      <tr>
        <td><%= survey_result.email %></td>
        <td><%= survey_result.name %></td>
        <td><%= survey_result.title %></td>
        <td><%= survey_result.company %></td>
        <td><%= survey_result.phone %></td>
        <td><%= link_to 'Show', survey_result %></td>
        <td><%= link_to 'Edit', edit_survey_result_path(survey_result) %></td>
        <td><%= link_to 'Destroy', survey_result, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Survey Result', new_survey_result_path %>
